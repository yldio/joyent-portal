{
  consul: 'consul:8500',
  jobs: [
    {
      name: 'setup-config',
      exec: '/bin/prestart.sh',
      when: {
        source: 'watch.rethinkdb',
        once: 'healthy'
      }
    },
    {
      name: 'tests',
      exec: 'node node_modules/.bin/lab -c -m 10000',
      when: {
        source: 'setup-config',
        once: 'exitSuccess'
      }
    }
  ],
  watches: [
    {
      name: 'rethinkdb',
      interval: 5
    },
    {
      name: 'docker-compose-api',
      interval: 5
    }
  ]
}
